<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>

  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body id="<%= params[:action] %>" class="<%= params[:controller] %>">

<%= render "layouts/nav" %>
  
  <div class="container">
    <div class="row">
      <div class="span12">  
        <%= yield %>
      </div>
    </div>
    
  </div><!-- end container -->
</body>
</html>